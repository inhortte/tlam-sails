<br class="clear" />
<table class="table table-striped table-condensed">
  <thead>
    <tr>
      <th>&nbsp;</th>
      <th>&nbsp;</th>
      <th>&nbsp;</th>
      <th>Name</th>
      <th>Species</th>
      <th>ID</th>
      <th>Sex</th>
      <th>B Date</th>
      <th>R Date</th>
      <th>N</th>
      <th>E</th>
    </tr>
  </thead>
  <tbody>
  {{#each}}
    <tr>
      <td><span class="glyphicon glyphicon-chevron-down"></span></td>
      <td><span class="glyphicon glyphicon-tree-conifer"></span></td>
      <td><span class="glyphicon glyphicon-file"></span></td>
      <td>{{#link-to 'animals.edit' id}}{{nickname}}{{/link-to}}</td>
      <td>{{species.name}}</td>
      <td>{{animal_id}}</td>
      <td>{{sex}}</td>
      <td>{{birthdate}}</td>
      <td>{{release_date}}</td>
      <td>{{release_location_N}}</td>
      <td>{{release_location_E}}</td>
    </tr>
  {{/each}}
  </tbody>
</table>
